{
  "name": "ember-tumblr",
  "version": "0.8.1",
  "description": "Ember Addon for integrating a Tumblr blog",
  "keywords": [
    "ember-addon",
    "tumblr",
    "blog"
  ],
  "license": "MIT",
  "author": "Jordan Hawker <hawker.jordan@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/elwayman02/ember-tumblr",
  "bugs": {
    "url": "https://github.com/elwayman02/ember-tumblr/issues"
